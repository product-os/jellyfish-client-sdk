{
  "name": "@balena/jellyfish-client-sdk",
  "version": "10.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-client-sdk.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "HTTP client SDK for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "autumndb": "^19.3.1",
    "axios": "^0.26.1",
    "common-tags": "^1.8.2",
    "deep-copy": "^1.4.2",
    "fast-json-patch": "^3.1.1",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.21",
    "socket.io-client": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.1",
    "@balena/jellyfish-types": "^2.0.4",
    "@balena/lint": "^6.2.0",
    "@types/common-tags": "^1.8.1",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/sinon": "^10.0.11",
    "@types/uuid": "^8.3.4",
    "deplint": "^1.1.3",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "nock": "^13.2.4",
    "simple-git-hooks": "^2.7.0",
    "sinon": "^13.0.2",
    "ts-jest": "^28.0.2",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-05-10T03:45:47.132Z"
  }
}
