{
  "name": "@balena/jellyfish-client-sdk",
  "version": "4.4.22",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-client-sdk.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "HTTP client SDK for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint-fix": "balena-lint --fix lib test",
    "unit": "jest",
    "test": "npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "common-tags": "^1.8.0",
    "deep-copy": "^1.4.2",
    "fast-json-patch": "^3.0.0-1",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.21",
    "socket.io-client": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-types": "^0.6.2",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.35",
    "@types/common-tags": "^1.8.0",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/sinon": "^10.0.2",
    "@types/socket.io-client": "^1.4.36",
    "@types/uuid": "^8.3.0",
    "depcheck": "^1.4.1",
    "deplint": "^1.1.3",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "nock": "^13.1.0",
    "simple-git-hooks": "^2.4.1",
    "sinon": "^11.1.1",
    "ts-jest": "^27.0.2",
    "typedoc": "^0.20.36",
    "typescript": "^4.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
